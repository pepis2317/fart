generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model msArtwork {
  ArtworkID          String    @id(map: "PK__msArtwor__D073AEBB8950E82D") @default(cuid()) @db.VarChar(50)
  ArtworkImage       String?   @db.VarChar(255)
  ArtworkDescription String?   @db.VarChar(200)
  ArtworkDate        DateTime? @db.Date
  UserID             String?   @db.VarChar(50)
  ArtworkTitle       String?   @db.VarChar(255)
  msUser             msUser?   @relation(fields: [UserID], references: [UserID], onDelete: NoAction, onUpdate: NoAction, map: "FK__msArtwork__UserI__4BAC3F29")
}

model msUser {
  UserID        String      @id(map: "PK__msUser__1788CCACC5DD6DAB") @default(cuid()) @db.VarChar(50)
  Username      String      @db.VarChar(50)
  UserEmail     String      @db.VarChar(50)
  UserPassword  String      @db.VarChar(50)
  UserPfp       String?     @db.VarChar(255)
  UserBanner    String?     @db.VarChar(255)
  Commissioning Int?
  About         String?     @db.VarChar(200)
  msArtwork     msArtwork[]
}
model msAss{
  assID      String      @id(map: "PK__") @default(cuid()) @db.VarChar(50)
  
}
